{
  "name": "@xpress-core/fs-routes",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "build": "wireit",
    "dev": "tsup --watch"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "wireit": {
    "build": {
      "command": "tsup",
      "files": [
        "*.ts",
        "tsconfig.json",
        "package.json"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "peerDependencies": {
    "@react-router/dev": "catalog:",
    "react": "catalog:"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "minimatch": "^9.0.0"
  },
  "devDependencies": {
    "@react-router/dev": "catalog:",
    "react": "catalog:",
    "tsup": "^8.3.0",
    "typescript": "^5.1.6",
    "wireit": "0.14.9"
  }
}
