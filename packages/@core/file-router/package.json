{
  "name": "@xpress-core/file-router",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm typecheck && pnpm unbuild",
    "stub": "unbuild --stub",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    }
  },
  "dependencies": {
    "chokidar": "3.6.0",
    "consola": "catalog:",
    "fast-glob": "^3.3.3",
    "gradient-string": "2.0.2",
    "kolorist": "^1.8.0",
    "magicast": "0.3.4",
    "micromatch": "4.0.7",
    "prettier": "3.3.3",
    "recast": "0.23.9",
    "unplugin": "1.12.0"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.6",
    "@types/micromatch": "4.0.9",
    "@types/node": "catalog:",
    "react-router-dom": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:"
  }
}
