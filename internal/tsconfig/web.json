{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Web Package",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "useDefineForClassFields": true,
    "moduleResolution": "bundler",
    "types": ["vite/client"],
    "allowJs": true,
    "declaration": false,
    "esModuleInterop": true,
    "isolatedModules": true
  }
}
